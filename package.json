{
  "name": "ganttcharty",
  "version": "1.0.0",
  "description": "simple ganttchart library.",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --config=webpack.config.build.js",
    "debug": "webpack serve --config=webpack.config.debug.js",
    "test": "jest",
    "watch": "run-p watch:*",
    "watch:test": "jest --watch",
    "watch:build": "webpack --watch",
    "prettier": "prettier --write './{src,test}/**/*.{ts,tsx}'",
    "pretty-quick": "pretty-quick",
    "precommit": "pretty-quick --staged",
    "postcommit": "git reset"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json",
        "diagnostics": true
      }
    },
    "testMatch": [
      "<rootDir>/(src|test)/**/*.test.+(ts|tsx|js)"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-react-constant-elements": "^7.13.13",
    "@babel/plugin-transform-react-inline-elements": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/classnames": "^2.3.1",
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.170",
    "@types/lodash-es": "^4.17.4",
    "@types/memoize-one": "^5.1.2",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "@types/react-icons": "^3.0.0",
    "@types/react-resize-detector": "^6.1.0",
    "@types/styled-components": "^5.1.9",
    "@types/uuid": "^8.3.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "core-js": "^3.13.0",
    "husky": "^7.0.1",
    "jest": "^27.0.1",
    "lodash-es": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.0",
    "snapshot-diff": "^0.9.0",
    "ts-jest": "^27.0.1",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "classnames": "^2.3.1",
    "closest-element": "0.0.3",
    "date-fns": "^2.22.0",
    "hotkeys-js": "^3.8.5",
    "immer": "^9.0.5",
    "lodash": "^4.17.21",
    "memoize-one": "^5.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-full-screen": "^1.0.2",
    "react-icons": "^4.2.0",
    "react-resize-detector": "^6.7.2",
    "state-use": "^5.0.0",
    "styled-components": "^5.3.0",
    "tslib": "^2.2.0",
    "uuid": "^8.3.2"
  }
}
