{
  "name": "ganttcharty",
  "version": "1.0.0",
  "description": "simple ganttchart library.",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "debug": "webpack-dev-server",
    "test": "jest",
    "watch": "run-p watch:*",
    "watch:test": "jest --watch",
    "watch:build": "webpack --watch"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json",
        "diagnostics": true
      }
    },
    "testMatch": [
      "<rootDir>/(src|test)/**/*.test.+(ts|tsx|js)"
    ]
  },
  "devDependencies": {
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^23.3.5",
    "@types/lodash": "^4.14.117",
    "@types/memoize-one": "^3.1.1",
    "@types/react": "^16.4.15",
    "@types/react-dom": "^16.0.8",
    "@types/react-resize-detector": "^3.1.0",
    "@types/styled-components": "^3.0.1",
    "@types/uuid": "^3.4.4",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.3",
    "snapshot-diff": "^0.4.0",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.2.1",
    "typescript": "^3.1.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "closest-element": "0.0.3",
    "date-fns": "^1.29.0",
    "hotkeys-js": "^3.3.8",
    "lodash": "^4.17.11",
    "memoize-one": "^4.0.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-resize-detector": "^3.1.3",
    "srimmer": "^0.4.0",
    "styled-components": "^3.4.9",
    "tslib": "^1.9.3",
    "uuid": "^3.3.2"
  }
}
