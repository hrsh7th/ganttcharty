{
  "name": "ganttcharty",
  "version": "1.0.0",
  "description": "simple ganttchart library.",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --config=webpack.config.build.js",
    "debug": "webpack-dev-server --config=webpack.config.debug.js",
    "test": "jest",
    "watch": "run-p watch:*",
    "watch:test": "jest --watch",
    "watch:build": "webpack --watch",
    "prettier": "prettier --write './{src,test}/**/*.{ts,tsx}'",
    "pretty-quick": "pretty-quick",
    "precommit": "pretty-quick --staged",
    "postcommit": "git reset"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json",
        "diagnostics": true
      }
    },
    "testMatch": [
      "<rootDir>/(src|test)/**/*.test.+(ts|tsx|js)"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-react-constant-elements": "^7.2.0",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/classnames": "^2.2.7",
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^24.0.13",
    "@types/lodash": "^4.14.132",
    "@types/lodash-es": "^4.17.3",
    "@types/memoize-one": "^4.1.1",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-icons": "^3.0.0",
    "@types/react-resize-detector": "^4.0.1",
    "@types/styled-components": "^4.1.15",
    "@types/uuid": "^3.4.4",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.0",
    "core-js": "^2.6.9",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "lodash-es": "^4.17.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.11.0",
    "snapshot-diff": "^0.5.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5",
    "webpack": "^4.32.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "classnames": "^2.2.6",
    "closest-element": "0.0.3",
    "date-fns": "^1.30.1",
    "hotkeys-js": "^3.6.11",
    "lodash": "^4.17.11",
    "memoize-one": "^5.0.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-full-screen": "^0.2.4",
    "react-icons": "^3.7.0",
    "react-resize-detector": "^4.1.3",
    "srimmer": "^0.6.0",
    "styled-components": "^4.2.0",
    "tslib": "^1.9.3",
    "uuid": "^3.3.2"
  }
}
